---
apiVersion: v1
kind: Service
metadata:
  labels:
    app: frontend
  name: frontend-lb
  namespace: default
spec:
  deprecatedPublicIPs:
  - {{ ansible_default_ipv4.address }}
  externalIPs:
  - {{ ansible_default_ipv4.address }}
  ports:
  - name: port-1
    port: 80
    protocol: TCP
    targetPort: 80
  - name: port-2
    port: 443
    protocol: TCP
    targetPort: 443
  selector:
    app: frontend
  sessionAffinity: None
  type: ClusterIP
status:
  loadBalancer: {}
